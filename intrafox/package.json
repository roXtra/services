{
  "name": "@eformservice/intrafox",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "servicename": "intrafox"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./",
    "test": "cross-env HOTRELOAD=no DEBUG=bpmn-engine:123* mocha ./disttests/**/*.roxtest.js --exit --recursive --timeout 40000 --require source-map-support/register",
    "build": "rimraf ./disttests && tsc && npm run webpack && npm run copymain",
    "copyfiles": "cross-var copyfiles service.json configtemplate.json dist/* $npm_package_config_servicename",
    "zip": "cross-var bestzip $npm_package_config_servicename.zip $npm_package_config_servicename",
    "copyandzip": "cross-var npm run copyfiles && npm run zip && cross-var rimraf $npm_package_config_servicename",
    "webpack": "cross-var webpack --env servicename=$npm_package_config_servicename --config ../webpack.config.js",
    "copymain": "copyfiles -u 1 dist/main.js disttests"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dateformat": "^5.0.3",
    "node-fetch": "^2.6.9",
    "processhub-sdk": "https://github.com/roXtra/processhub-sdk/releases/download/v9.62.0/release.tgz",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/dateformat": "^5.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.28",
    "bestzip": "^2.2.1",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "rimraf": "^4.4.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
