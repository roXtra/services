{
  "name": "@eformservice/sap",
  "servicename": "sap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./",
    "build": "tsc",
    "test": "cross-env HOTRELOAD=no DEBUG=bpmn-engine:123* mocha ./dist/**/*.roxtest.js --exit --recursive --timeout 40000 --require source-map-support/register",
    "copyproduction": "cross-var copyfiles package.json production && copyfiles .npmrc production && cd ./production && npm install --production && cd .. && cross-var copyfiles -u 1 \"./production/node_modules/**/*\" $npm_package_servicename && rimraf production/",
    "copyfiles": "cross-var copyfiles service.json dist/* $npm_package_servicename && npm run copyproduction",
    "zip": "cross-var bestzip $npm_package_servicename.zip $npm_package_servicename",
    "copyandzip": "cross-var npm run copyfiles && npm run zip && cross-var rimraf $npm_package_servicename"
  },
  "engines": {
    "node": "^v14.17.6",
    "npm": "^6.14.15"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sap/hana-client": "^2.9.28",
    "processhub-sdk": "https://github.com/roXtra/processhub-sdk/releases/download/v9.23.0-4/release.tgz",
    "react": "^17.0.2",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.15",
    "@types/react": "^17.0.20",
    "bestzip": "^2.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^7.32.0",
    "mocha": "^9.1.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2"
  }
}
