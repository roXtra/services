{
  "name": "@eformservice/sap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "servicename": "sap"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./",
    "build": "tsc",
    "test": "cross-env HOTRELOAD=no DEBUG=bpmn-engine:123* mocha ./dist/**/*.roxtest.js --exit --recursive --timeout 40000 --require source-map-support/register",
    "copyproduction": "cross-var copyfiles package.json production && copyfiles .npmrc production && cd ./production && npm install --production && cd .. && cross-var copyfiles -u 1 \"./production/node_modules/**/*\" $npm_package_config_servicename && rimraf production/",
    "copyfiles": "cross-var copyfiles service.json dist/* $npm_package_config_servicename && npm run copyproduction",
    "zip": "cross-var bestzip $npm_package_config_servicename.zip $npm_package_config_servicename",
    "copyandzip": "cross-var npm run copyfiles && npm run zip && cross-var rimraf $npm_package_config_servicename"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sap/hana-client": "^2.13.21",
    "processhub-sdk": "https://github.com/roXtra/processhub-sdk/releases/download/v9.44.0-9/release.tgz",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-react": "^2.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.43",
    "@types/react": "^17.0.47",
    "bestzip": "^2.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.19.0",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4"
  }
}
