{
  "name": "@eformservice/roxfile",
  "servicename": "roxFile",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./",
    "test": "cross-env HOTRELOAD=no DEBUG=bpmn-engine:123* mocha ./disttests/**/*.roxtest.js --exit --recursive --timeout 40000 --require source-map-support/register",
    "build": "rimraf ./disttests && tsc && npm run webpack && npm run copymain",
    "copyfiles": "cross-var copyfiles service.json dist/* $npm_package_servicename",
    "zip": "cross-var bestzip $npm_package_servicename.zip $npm_package_servicename",
    "copyandzip": "cross-var npm run copyfiles && npm run zip && cross-var rimraf $npm_package_servicename",
    "webpack": "cross-var webpack --env servicename=$npm_package_servicename --config ../webpack.config.js",
    "copymain": "copyfiles -u 1 dist/main.js disttests"
  },
  "engines": {
    "node": "^v14.18.2",
    "npm": "^6.14.15"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/json-query": "^2.2.3",
    "json-query": "^2.2.2",
    "node-fetch": "^2.6.6",
    "processhub-sdk": "https://github.com/roXtra/processhub-sdk/releases/download/v9.31.0-3/release.tgz",
    "react": "^17.0.2",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.38",
    "bestzip": "^2.2.0",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^7.32.0",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
