{
  "name": "@eformservice/roxfile",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "servicename": "roxFile"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./",
    "test": "cross-env HOTRELOAD=no DEBUG=bpmn-engine:123* mocha ./disttests/**/*.roxtest.js --exit --recursive --timeout 40000 --require source-map-support/register",
    "build": "rimraf ./disttests && tsc && npm run webpack && npm run copymain",
    "copyfiles": "cross-var copyfiles service.json dist/* $npm_package_config_servicename",
    "zip": "cross-var bestzip $npm_package_config_servicename.zip $npm_package_config_servicename",
    "copyandzip": "cross-var npm run copyfiles && npm run zip && cross-var rimraf $npm_package_config_servicename",
    "webpack": "cross-var webpack --env servicename=$npm_package_config_servicename --config ../webpack.config.js",
    "copymain": "copyfiles -u 1 dist/main.js disttests"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/json-query": "^2.2.3",
    "json-query": "^2.2.2",
    "node-fetch": "^2.6.7",
    "processhub-sdk": "https://github.com/roXtra/processhub-sdk/releases/download/v9.38.0/release.tgz",
    "react": "^17.0.2",
    "semantic-ui-react": "^2.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.27",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^17.0.44",
    "bestzip": "^2.2.0",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.13.0",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
